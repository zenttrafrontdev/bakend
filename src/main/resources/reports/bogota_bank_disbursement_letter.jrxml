	<queryString language="plsql">
		<![CDATA[select 
p.nombre_proyecto as nombreProyecto,
valor,
t.nombre,
t.identificacion,
cuenta_destino as cuentaDestino,
c.plazo,
t2.nombre as tasa,
ac.NOMBRE as capital,
pi2.nombre as intereses,
(select cast(cast(sum(valor) as UNSIGNED) as CHAR) from desembolsos d where $P!{where}) as valorTotal,
(select cast(cast(sum(valor_gmf) as UNSIGNED) as CHAR) from desembolsos d where $P!{where}) as valorGmf
from 
desembolsos des
inner join proyectos p on p.id = des.proyecto_id
inner join terceros t on t.id = des.tercero_id 
inner join cupos c on c.id = des.cupo_id 
inner join tasas t2 on t2.id = c.fee_id 
inner join amortizaciones_capital ac on ac.ID = c.amortizacion_capital_id 
inner join periodicidad_intereses pi2 on pi2.id = c.periodicidad_interes_id 
where $P!{where}]]>
	</queryString>
