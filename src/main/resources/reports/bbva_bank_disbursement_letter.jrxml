	<queryString language="plsql">
		<![CDATA[select
p.nombre_proyecto as nombreProyecto,
d.valor,
t.nombre,
t.id_contribuyente as identificacion,
(select cast(cast(sum(valor) as UNSIGNED) as CHAR) from desembolsos d where $P!{where}) as valorTotal,
(select cast(cast(sum(valor_gmf) as UNSIGNED) as CHAR) from desembolsos d where $P!{where}) as valorGmf,
d.cuenta_destino as cuentaDestino
from grupos_desembolso gd 
inner join desembolsos d on d.grupo_desembolso_id = gd.id
inner join proyectos p on p.id = gd.proyecto_id
inner join terceros t on t.id = d.tercero_id
where $P!{where}]]>
	</queryString>
