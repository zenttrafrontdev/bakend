	<queryString language="plsql">
		<![CDATA[select 
p.nombre_proyecto as nombreProyecto,
valor,
t.nombre,
t.identificacion,
(select cast(cast(sum(valor) as UNSIGNED) as CHAR) from desembolsos d where $P!{where}) as valorTotal,
(select cast(cast(sum(valor_gmf) as UNSIGNED) as CHAR) from desembolsos d where $P!{where}) as valorGmf
from 
desembolsos des
inner join proyectos p on p.id = des.proyecto_id
inner join terceros t on t.id = des.tercero_id 
where $P!{where}]]>
	</queryString>
